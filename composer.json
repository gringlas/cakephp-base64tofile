    "type": "cakephp-plugin",
    "require": {
        "php": ">=5.5.9",
        "cakephp/cakephp": ">=3.3.2 <4.0.0"
    },
    "authors" : [
        {
            "name" : "Sebastian KÃ¶ller",
            "role" : "Author",
            "homepage" : "http://www.phihochzwei.com"
        }
    ],
    "require-dev": {
        "phpunit/phpunit": "*"
    },
    "autoload": {
        "psr-4": {
            "Base64ToFile\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "DHLApi\\Test\\": "tests",
            "Cake\\Test\\": "./vendor/cakephp/cakephp/tests"
        }
    }
}
